<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
	<policies>
		<policy id="connect-src">
			<values>
				<value id="loqate-api" type="host">api.addressy.com</value>
			</values>
		</policy>
		<policy id="style-src">
            <values>
                <value id="capture-css" type="host">api.addressy.com</value>
            </values>
        </policy>
		<policy id="script-src">
            <values>
                <value id="capture-js" type="host">api.addressy.com</value>
            </values>
        </policy>
		<policy id="img-src">
            <values>
                <value id="capture-images" type="host">services.postcodeanywhere.co.uk</value>
            </values>
        </policy>
	</policies>
</csp_whitelist>
